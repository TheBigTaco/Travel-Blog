@model IEnumerable<TravelBlog.Models.Experience>

@{
    ViewData["Title"] = "Experiences";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Experience List</h1>
@foreach (var experience in Model)
{
    <p>@experience.title</p>
    <p>@Html.ActionLink("Edit", "Edit", new { id = experience.id }) |
       @Html.ActionLink("Delete", "Delete", new { id = experience.id })</p>
}

<p>@Html.ActionLink("Back Home", "Index", "Location")</p>